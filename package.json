{
  "name": "hydra",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node ace build --production",
    "dev": "node ace serve --watch",
    "start": "node server.js",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build",
    "ui",
    "node_modules"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.3",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.2.1",
    "@types/lodash": "^4.14.186",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "youch": "^3.2.2",
    "youch-terminal": "^2.1.5"
  },
  "dependencies": {
    "@adonisjs/core": "^5.8.7",
    "@adonisjs/lucid": "^18.2.0",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/view": "^6.2.0",
    "@supercharge/promise-pool": "^2.3.2",
    "axios": "^1.1.3",
    "cheerio": "^1.0.0-rc.12",
    "knex-postgis": "^0.14.3",
    "lodash": "^4.17.21",
    "luxon": "^3.0.4",
    "pg": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21"
  }
}
