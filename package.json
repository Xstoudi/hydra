{
  "name": "hydra",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node ace build --production",
    "dev": "node ace serve --watch",
    "start": "node server.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "vite:dev": "vite",
    "vite:build": "vite build"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build",
    "ui",
    "node_modules"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.3",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.2.1",
    "@types/leaflet": "^1.9.0",
    "@types/lodash": "^4.14.186",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "adonis-preset-ts": "^2.1.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.0.0",
    "install": "^0.13.0",
    "npm": "^9.4.0",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.4",
    "ts-loader": "^9.0.0",
    "typescript": "^4.6.4",
    "vite": "^4.1.1",
    "youch": "^3.2.2",
    "youch-terminal": "^2.1.5"
  },
  "dependencies": {
    "@91codes/adonis-vite": "^0.3.2",
    "@adonisjs/core": "^5.8.7",
    "@adonisjs/i18n": "^1.5.6",
    "@adonisjs/lucid": "^18.2.0",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/view": "^6.2.0",
    "@eidellev/inertia-adonisjs": "^7.4.0",
    "@headlessui/react": "^1.7.8",
    "@heroicons/react": "^2.0.14",
    "@inertiajs/inertia": "^0.11.1",
    "@inertiajs/inertia-react": "^0.8.1",
    "@inertiajs/react": "^1.0.0",
    "@inertiajs/server": "^0.1.0",
    "@supercharge/promise-pool": "^2.3.2",
    "@tanstack/react-query": "^4.23.0",
    "@vitejs/plugin-react": "^3.1.0",
    "axios": "^1.1.3",
    "cheerio": "^1.0.0-rc.12",
    "clsx": "^1.2.1",
    "i18next": "^22.4.9",
    "i18next-browser-languagedetector": "^7.0.1",
    "knex-postgis": "^0.14.3",
    "leaflet": "^1.9.3",
    "lodash": "^4.17.21",
    "luxon": "^3.0.4",
    "pg": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "proxy-addr": "^2.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.4",
    "react-leaflet": "^3.2.5",
    "react-toastify": "^9.1.1",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "twemoji": "^14.0.2"
  }
}
